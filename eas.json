{
  "cli": {
    "version": ">=10.0.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "ios": {
        "simulator": false,
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
